@section jsScripts{
<script>
        var SubButton = document.getElementById('submitMail');
        var UserMail = document.getElementById('user_email');
        const token = document.querySelector('input[name="__RequestVerificationToken"]').value;

        SubButton.addEventListener(
            'click',
            function (e) { 
                e.preventDefault(); 
                submitMail();
            }
        );
    function submitMail()
    {
            
            var data = {
                EMailId: UserMail.value
            };
            axios.defaults.headers.common['RequestVerificationToken'] = token;
            axios.post('/api/newsletter', data)
                .then(function (response) {
                    console.log(response)
                        toaster("success",response.data);  
                })
                .catch(function (error) {
                    console.log(error);
                    toaster("error", error.response.data);

                })

    }


    function addblgs() {
        let innerArticle = "";
            innerArticle += ` <article class="swiper-slide h-auto swiper-slide-next" role="group" aria-label="2 / 3" style="width: 416px; margin-right: 24px;">
